{
  "name": "zenless-club-remix-9688",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "cross-env NODE_ENV=production npm run generate:css -- --verbose",
    "build:remix": "remix build",
    "build:server": "node build.mjs",
    "dev": "run-s build:* && run-p dev:*",
    "dev:build": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch --verbose",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development node --inspect --require ./node_modules/dotenv/config ./build/server.js",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "generate:css": "postcss ./app/styles/index.css -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node --require ./node_modules/dotenv/config ./build/server.js",
    "typecheck": "tsc -b",
    "validate": "run-p lint typecheck"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
